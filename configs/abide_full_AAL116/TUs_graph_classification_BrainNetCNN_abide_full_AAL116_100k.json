{
    "gpu": {
        "use": true,
        "id": 0
    },
    
    "model": "BrainNetCNN",
    "dataset": "abide_full_AAL116",
    "views": ["abide_full_AAL116"],
    
    "out_dir": "out/braindata_graph_classification/",

    "params": {
        "seed": 41,
        "epochs": 1000,
        "batch_size": 20,
        "init_lr": 7e-4,
        "lr_reduce_factor": 0.5,
        "lr_schedule_patience": 25,
        "min_lr": 1e-6,
        "weight_decay": 0.0,
        "print_epoch_interval": 5,
        "max_time": 12
    },
    
    "net_params": {
        "hidden_dim": 64,
        "out_dim": 256
    }
}